package kosaShoppingMall.service.empJoin;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;
import org.springframework.stereotype.Service;

import kosaShoppingMall.mapper.EmpShipMapper;
import kosaShoppingMall.mapper.MemberShipMapper;

@Component
@Service
public class EmpDropService {
	@Autowired
	EmployeeShipMapper empShipMapper;
	public void execute(String empId) {
		empShipMapper.empDrop(empId);
	}
}