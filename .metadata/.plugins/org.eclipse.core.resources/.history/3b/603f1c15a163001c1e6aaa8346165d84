	var std = 3
	var edd = 6
	var stg = 4
	var edg = 7
	var dan = std
	while(dan <= edd) {
		var gop = stg
		while(gop <= edg) {
		document.write(dan + " X " + gop + " = " + (dan*gop) + "<br/>")
		gop++
		}
		dan++
	}
</script>
